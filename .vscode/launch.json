    "version": "0.2.0",
    "configurations": [
        {
            "name": "Repetition .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Repetition",
            "program": "${workspaceFolder}/base56Repetition/bin/Debug/netcoreapp3.0/base56Repetition.dll",
            "args": ["/media/cash/d/", "8388608"],
            "cwd": "${workspaceFolder}/base56Repetition",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Generator .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Generator",
            "program": "${workspaceFolder}/base56Generator/bin/Debug/netcoreapp3.0/base56Generator.dll",
            "args": ["guid.bin","2097152"],
            "cwd": "${workspaceFolder}/base56Generator",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Calculator .NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Calculator",
            "program": "${workspaceFolder}/base56Calculator/bin/Debug/netcoreapp3.0/base56Calculator.dll",
            "args": ["/media/cash/d/"],
